<Window x:Class="Process_PC_Manager.ListNamePromptWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Height="125" Width="225" Background="LightGray" WindowStyle="None">
    <Grid>
        <TextBox x:Name="ListName" HorizontalAlignment="Left" Height="23" Margin="87,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="List Name:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button Content="Enter" HorizontalAlignment="Left" Margin="10,85,0,0" VerticalAlignment="Top" Width="75" Click="Enter_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="132,85,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
        <CheckBox x:Name="Copy" Content="Copy Current Configuration" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
